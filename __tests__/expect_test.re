open Jest;
open Expect;
open Puzzles;

let description = (method, input, expected) =>
  method ++ " expected=" ++ expected;

type input('a, 'b, 'c) =
  | Input('a, 'b)
  | PuzzleInput('a, 'b, 'c);

let runTest = (fn, fnName, case) =>
  switch (case) {
  | Input(input, expected) =>
    test(description(fnName, input, string_of_int(expected)), () =>
      expect(fn(input)) |> toBe(expected)
    )
  | PuzzleInput(input, expected, short) =>
    test(description(fnName, short, string_of_int(expected)), () =>
      expect(fn(input)) |> toBe(expected)
    )
  };

